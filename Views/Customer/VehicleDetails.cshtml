@model CarRentalService.Models.Vehicle

@{
    ViewData["Title"] = "VehicleDetails";
}

<h1>@Model.Year  @Model.Make @Model.Model</h1>

<div class="row">
    <div class="col-6"><img style="width:100%;" src="@Url.Content(@Model.Image)" /></div>
    <div class="col-6"><img src="@ViewBag.MapUrl" /></div>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
